{
	"name": "template",
	"version": "0.0.0",
	"description": "Template",
	"main": "server.js",
	"author": {
		"name": "Julien MERASLI",
		"url": "https://julienmerasli.vercel.app",
		"email": "julien.merasli@hotmail.com"
	},
	"repository": "github:JulienMERASLI/Node-template",
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon -L --ignore 'public/' --ext 'js' server.js",
		"tscServer": "ttsc --watch -p tsconfig.server.json",
		"tscClient": "cd public && ttsc --watch -p tsconfig.client.json",
		"postinstall": "(ttsc -p tsconfig.server.json) && (cd public && ttsc -p tsconfig.client.json) && (for lessfile in $(find public/styles -name '*.less'); do cssfile=$(echo $lessfile | sed 's/\\.less$/.css/i'); echo \"Compiling $lessfile to $cssfile\"; rm -f $cssfile; lessc $lessfile > $cssfile; done;)"
	},
	"keywords": [
		"Template"
	],
	"license": "ISC",
	"dependencies": {
		"async": "^3.2.0",
		"bcryptjs": "^2.4.3",
		"dom-chef": "^5.0.0",
		"dotenv": "^8.2.0",
		"ejs": "^3.1.5",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"express-session": "^1.17.1",
		"helmet": "^4.2.0",
		"joi": "^17.4.0",
		"mongoose": "^5.10.17",
		"morgan": "^1.10.0",
		"node-fetch": "^2.6.1",
		"nodemailer": "^6.4.16",
		"passport": "^0.4.1",
		"passport-local": "^1.0.0",
		"serve-favicon": "^2.5.0",
		"ttypescript": "^1.5.12",
		"typescript-is": "^0.18.2"
	},
	"devDependencies": {
		"@types/async": "^3.2.4",
		"@types/bcryptjs": "^2.4.2",
		"@types/express": "^4.17.9",
		"@types/express-session": "^1.17.3",
		"@types/morgan": "^1.9.2",
		"@types/node": "^14.14.10",
		"@types/node-fetch": "^2.5.7",
		"@types/nodemailer": "^6.4.0",
		"@types/passport": "^1.0.4",
		"less": "^4.0.0",
		"typed-query-selector": "^2.4.1",
		"typescript": "^4.1.2"
	}
}
